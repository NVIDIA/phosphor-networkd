[Match]
Type=ether
[Network]
DHCP=true
LinkLocalAddressing=@LINK_LOCAL_AUTOCONFIGURATION@
IPv6AcceptRA=@ENABLE_IPV6_ACCEPT_RA@
[DHCP]
ClientIdentifier=mac
VendorClassIdentifier=@VENDOR_CLASS_ID@
UseDNS=true
UseDomains=true
UseNTP=true
UseHostname=true
SendHostname=true
[IPv6AcceptRA]
DHCPv6Client=true
[DHCPv6]
VendorClass=@VENDOR_CLASS_ID@
